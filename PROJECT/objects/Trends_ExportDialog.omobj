	<object access-modifier="private" name="SaveMode" display-name="SaveMode" uuid="be192bc9-0983-4405-8cfb-994ac120edb4" base-type="notifying_int4" base-type-id="5ccca629-a45d-46c8-a56c-83ecde8126f7" ver="3">
		<do-on access-modifier="private" name="Handler_1" display-name="Handler_1" ver="4" event="ValueChanged">
			<body kind="om">
				<![CDATA[fname:string= "trends"+pTrends.Main_Panel.private.DateTimeToFileName(DateTime.Now(),"zip");		// получаем имя файла из текущих даты и времени

if (pTrends.ExportImportFolderPath != "")		// Если пользователь задал корневую директорию
{
	if (pTrends.FileManager.Exists(pTrends.ExportImportFolderPath))
		pTrends.FileManager.CreateRecursiveFolder(pTrends.ExportImportFolderPath);
	recEditPath.recRootPath.Text_1 = pTrends.FileManager.CombinePath(pTrends.ExportImportFolderPath, "_");	// добавляем слэш в конце
	recEditPath.recRootPath.Text_1 = Str.Remove(recEditPath.recRootPath.Text_1, Str.Length(recEditPath.recRootPath.Text_1)-1, 1);
															// из текущей папки эспорта извлекаем корневую часть, остальное помещаем в поле для редактирования
	recEditPath.TextEdit_1.Text = Str.SubString(pTrends.Main_Panel.private.ExportPath, Str.Length(recEditPath.recRootPath.Text_1));
}
else													// Если используется директоря по умолчанию в папке проекта
{															// корневая часть остается пустой
	recEditPath.recRootPath.Text_1 = "";
	recEditPath.TextEdit_1.Text = pTrends.Main_Panel.private.ExportPath;		// весь путь экспорта помещается в поле для редактирования
}

recEditPath.recRootPath.LayoutWeight_1 = Str.Length(recEditPath.recRootPath.Text_1.Text)*7 +10;		// Размео поля корневого пути подстраивается под длину строки
recEditPath.TextEdit_1.LayoutWeight_1 = recEditPath.Width - recEditPath.recRootPath.LayoutWeight_1;	//Поле редактирования занимает оставшуюся часть

if (SaveMode == 0)		// Если сохраняем данные графиков
{
	Tab_1.bActive = true;
	Tab_2.bActive = false;
	recEditPath.TextEdit_1.Text = pTrends.FileManager.CombinePath(recEditPath.TextEdit_1.Text, fname);
}
else									// Если сохраняем список графиков
{
	Tab_1.bActive = false;
	Tab_2.bActive = true;
	recEditPath.TextEdit_1.Text = pTrends.FileManager.CombinePath(recEditPath.TextEdit_1.Text, "taglist");
	recEditPath.TextEdit_1.Text = pTrends.FileManager.CombinePath(recEditPath.TextEdit_1.Text, "TagList.json");
}]]>
			</body>
		</do-on>
	</object>
