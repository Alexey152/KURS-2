		<do-on access-modifier="private" name="Handler_1" display-name="Handler_1" ver="4" event="ButtonPressed">
			<body kind="javascript">
				<![CDATA[var fileName = recEditPath.GetPath();			// Извлекаем полное имя файла из строки ввода
if (SaveMode == 0)
{								// Сохранение данных
	pTrends.Main_Panel.private.ExportFile = fileName;		// Сохраняем имя файла для формирования конечного файл архива
	var j = fileName.lastIndexOf("/");									// 
	if (j < 0) j = fileName.lastIndexOf("\\");
	var filePath = fileName.substring(0,j);							// Получаем путь экспорта пользователя 
	pTrends.Main_Panel.private.ExportPath = filePath;		// Сохраняем его для последующего использования
		
	j = fileName.lastIndexOf(".");											// Используем имя файла без расширения для создания временнной папки 
	filePath = fileName.substring(0,j);									// для хранения набора файлов с данными графиков
	pTrends.Main_Panel.private.ExportTrendsPath = filePath;		// Переменную использует ExportLines
	pTrends.Main_Panel.private.ExportLines();
}
else
{								// Сохранение списка сигналов
	if (pTrends.SaveSignalsList(fileName)){
		pTrends.Main_Panel.private.MessagesClear();
		pTrends.Main_Panel.private.MessageAdd("Список сигналов успешно сохранен!");
		pTrends.Main_Panel.private.ShowMessage.Invoke();
	}
}

Close();]]>
			</body>
		</do-on>
