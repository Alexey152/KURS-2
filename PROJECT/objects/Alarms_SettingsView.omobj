	<object access-modifier="private" name="bInit" display-name="bInit" uuid="a3a67675-1d77-41fe-8d25-6f9879536880" base-type="bool" base-type-id="7c9fe1ab-1ee2-42d8-939f-94ce3e89fca8" ver="2"/>
	<init target="bInit" ver="2" value="false"/>
	<do-on access-modifier="private" name="OnOpened" display-name="OnOpened" ver="4" event="Opened">
		<body kind="javascript">
			<![CDATA[//Включение сортировки по порядку столбцов
Table_1.IndexColumn.SortOrder = 0;
Table_1.IndexColumn.SortPriority = 1;
Table_1.IndexColumn.SortingEnabled = true;
Table_1.ApplySort();

//Определяем положение CheckBox
for (let i = 0; i < VisibleCheckBoxArray.Length; i++)
{
	// Корректировка положения CheckBox
	VisibleCheckBoxArray[i].X = Table_1.X + Table_1.DescriptionColumn.Width + Table_1.TitleColumn.Width + (Table_1.ViewColumn.Width/2 - VisibleCheckBoxArray[0].Width/2);
	VisibleCheckBoxArray[i].Y = Table_1.Y + Table_1.HeaderHeight + 3 + (Table_1.RowHeight)* i;
	if (Table_1.TableModel.GetCellData(Table_1.MapRowToModel(i),2)) VisibleCheckBoxArray[i].State = true;
}

// Заполнение полей размеров
RowHeightBox.RowHeightEdit.Text = pSettingsForm.Settings.View.RowsHeigth;
TitleHeightBox.TitleHeightEdit.Text = pSettingsForm.Settings.View.TitleHeight;
FontSizeBox.FontSizeEdit.Text = pSettingsForm.Settings.View.Font.split(",")[1];
FontSizeTitleBox.FontSizeTitleEdit.Text = pSettingsForm.Settings.View.FontTitle.split(",")[1];

//Проверяем наличие ошибок
if ((pSettingsForm.errorWord & 0x2) == 0x2) RowHeightBox.RecBorder_Alarm.Visible = true; //Ошибка высоты строки
if ((pSettingsForm.errorWord & 0x4) == 0x4) TitleHeightBox.RecBorder_Alarm.Visible = true; //Ошибка высоты заголовка
if ((pSettingsForm.errorWord & 0x8) == 0x8) FontSizeBox.RecBorder_Alarm.Visible = true; //Ошибка размера шрифта строки
if ((pSettingsForm.errorWord & 0x10) == 0x10) FontSizeTitleBox.RecBorder_Alarm.Visible = true; //Ошибка размера шрифта заголовка
CheckErrorTable();//Проверка ошибок в таблице

bInit = true;//Разрешение на обработку формами своих значений
Focus = true; //Переводим Focus на форму для обработки нажатия клавиш

UpdateCheckBox(); // Обновление состояний VisibleCheckBox
UpdateButtonEnable();
pSettingsForm.CheckModify();
]]>
		</body>
	</do-on>
	<do-on access-modifier="private" name="Handler_2" display-name="Handler_2" ver="4" event="KeyPress">
		<body kind="om">
			<![CDATA[key: uint4 = event.KeyCode;
//DebugTool_1.Log("Form.KeyPress " + String.ToString(key));

if (key == 75) // Escape
	pSettingsForm.Close();
else if (key == 52) // F2
	Table_1.StartEditing();
]]>
		</body>
	</do-on>
