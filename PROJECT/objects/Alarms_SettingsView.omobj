	<param access-modifier="private" name="SettingsForm_1" display-name="SettingsForm_1" uuid="9c5775c6-028a-464f-b98c-3741a51c6d30" base-type="SettingsForm" base-type-id="82dc7618-130c-486c-840d-5d302f40306b" base-const="true" base-ref="true" ver="3"/>
	<do access-modifier="private" name="CheckWidthError" display-name="CheckWidthError" ver="4" type="bool" type-id="7c9fe1ab-1ee2-42d8-939f-94ce3e89fca8">
		<body kind="javascript">
			<![CDATA[//Функция проверка корректности ввода ширины столбца. Если true - ошибка
var v = Number.parseInt(inputString);
var mask1 = /[0-9]{1}\d{0,4}/; //регулярное выражения числа от 1 до 99999
//удаляем число из строки
var str = inputString.replace(mask1,"");
//если в строке что-то осталось или число - не число, то возвращаем true
if (str.length > 0 && str != "" || isNaN(v) || v < 0 || v > 65535) return true
return false;

]]>
		</body>
		<param access-modifier="private" name="inputString" display-name="" uuid="07996286-d352-4906-be5e-ad023f80dc27" base-type="string" base-type-id="76403785-f3d5-41a7-9eb6-d19d2aa2d95d" ver="3"/>
	</do>
	<do access-modifier="private" name="CheckErrorTable" display-name="CheckErrorTable" ver="3" type="" type-id="">
		<body kind="javascript">
			<![CDATA[//Функция проверки ошибок в таблице
pSettingsForm.errorWord = pSettingsForm.errorWord & 0xFFFE;

//Проверяем наличие ошибок в столбце ширины
for (let i = 0; i < Table_1.GetRowCount(); i++)
{
	ErrorIndicateArray[i].Visible = false;
	if (CheckWidthError(Table_1.GetCellData(i,3)))
	{
		ErrorIndicateArray[i].X = Table_1.Width - ErrorIndicateArray[i].Width - 2;
		ErrorIndicateArray[i].Y = Table_1.Y + Table_1.HeaderHeight + Table_1.RowHeight * i + 6;
		ErrorIndicateArray[i].Visible = true;
		pSettingsForm.errorWord = pSettingsForm.errorWord | 0x1;
	}
}



]]>
		</body>
	</do>
	<designed target="ZValue" value="0" ver="3"/>
	<designed target="Opacity" value="1" ver="3"/>
	<do access-modifier="private" name="UpdateCheckBox" display-name="UpdateCheckBox" ver="4" type="" type-id="">
		<body kind="om">
			<![CDATA[// Функция обновления состояния видимости столбцов
indexTimeGenState: int4 = -1;
indexTimeActState: int4 = -1;
indexMessageState: int4 = -1;

i: int4 = 0;
while (i < Table_1.GetRowCount())
{
	VisibleCheckBoxArray[i].State = Variant.ToBool(Table_1.TableModel.GetCellData(Table_1.MapRowToModel(i), 2), false);
	VisibleCheckBoxArray[i].Enabled = true;
	
	// Проверяем условия доступности чекбоксов для особых столбцов:
	if (Table_1.GetCellData(i, 0) == "Время генерации")
		indexTimeGenState = i;
	if (Table_1.GetCellData(i, 0) == "Время срабатывания")
		indexTimeActState = i;
	if (Table_1.GetCellData(i, 0) == "Сообщение")
		indexMessageState = i;
	
	i = i + 1;
}

if (VisibleCheckBoxArray[indexTimeGenState].State && !VisibleCheckBoxArray[indexTimeActState].State)
	VisibleCheckBoxArray[indexTimeGenState].Enabled = false;
	
if (!VisibleCheckBoxArray[indexTimeGenState].State && VisibleCheckBoxArray[indexTimeActState].State)
	VisibleCheckBoxArray[indexTimeActState].Enabled = false;
	
VisibleCheckBoxArray[indexMessageState].Enabled = false; // Столбец "Сообщение" всегда отображается
]]>
		</body>
	</do>
	<do access-modifier="private" name="SetVisibleValue" display-name="SetVisibleValue" ver="4" type="" type-id="">
		<body kind="om">
			<![CDATA[// Функция записи значения видимости столбца
Table_1.TableModel.SetCellData(Table_1.MapRowToModel(index), 2, VisibleCheckBoxArray[index].State);
UpdateCheckBox();
pSettingsForm.CheckModify();]]>
		</body>
		<param access-modifier="private" name="index" display-name="" uuid="5af9534c-9479-4eae-ac24-964aa7b30de5" base-type="int4" base-type-id="74222947-f188-4195-8e0e-e33bd9827255" ver="3"/>
	</do>
	<object access-modifier="private" name="pSettingsForm" display-name="pSettingsForm" uuid="489c06c2-589d-41cf-a92e-497460df5185" base-type="SettingsForm" base-type-id="82dc7618-130c-486c-840d-5d302f40306b" base-const="true" base-ref="true" ver="2"/>
	<do access-modifier="private" name="ShiftRow" display-name="ShiftRow" ver="4" type="" type-id="">
		<body kind="om">
			<![CDATA[//Функция сдвига строки вверх-вниз на указаное количество строк

nextRow: Table_1.curRow + offset; // Индекс следующей строки в таблице
modelCurRow: int4 = Table_1.MapRowToModel(Table_1.curRow); // Индекс текущей строки в модели
modelNextRow: int4 = Table_1.MapRowToModel(nextRow); // Индекс следующей строки в модели

// Получаем текущие значения индексов столбцов
nextRowIndex: int4 = Variant.ToInt4(Table_1.TableModel.GetCellData(modelNextRow, 4), -1);
currentRowIndex: int4 = Variant.ToInt4(Table_1.TableModel.GetCellData(modelCurRow, 4), -1);
//DebugTool_1.Log("Move row from " + String.ToString(currentRowIndex) + " to " + String.ToString(nextRowIndex));

if (nextRowIndex > -1 && currentRowIndex > -1)
{
	// Меняем индексы местами
	Table_1.TableModel.SetCellData(modelNextRow, 4, currentRowIndex);
	Table_1.TableModel.SetCellData(modelCurRow, 4, nextRowIndex);
	
	// Выделяем перенесенную строку
	Table_1.curRow = nextRow;
	Table_1.SetSelectedRows("[" + String.ToString(Table_1.curRow) + "]");
	UpdateButtonEnable();
	
	UpdateCheckBox();
	
	CheckErrorTable();
	pSettingsForm.CheckModify();
}]]>
		</body>
		<param access-modifier="private" name="offset" display-name="" uuid="a70ee622-1f7d-472b-ad22-08fef03c3c29" base-type="int4" base-type-id="74222947-f188-4195-8e0e-e33bd9827255" ver="3"/>
	</do>
	<do access-modifier="private" name="UpdateButtonEnable" display-name="UpdateButtonEnable" ver="4" type="" type-id="">
		<body kind="om">
			<![CDATA[isSelected: bool = Table_1.IsRowSelected(Table_1.curRow);
Button_Up.Enabled = isSelected && Table_1.curRow > 0;
Button_Down.Enabled = isSelected && Table_1.curRow < Table_1.GetRowCount() - 1;]]>
		</body>
	</do>
</type>

