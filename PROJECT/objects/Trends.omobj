			<do access-modifier="private" name="ExportLines" display-name="ExportLines" ver="4" type="" type-id="">
				<body kind="om">
					<![CDATA[	i:int4=1;
	fileName:string;
	tag:string;
	FileReadyCount = 0; // Обнуляем счетчик записанных файлов
	FileSaveCount = 0;  // Обнуляем счетчик файлов для записи
	FileLoadCount = 0;  // Обнуляем счетчик режима загрузки
			// Завершение процедуры записи отслеживается по изменению количества готовых записанных файлов
			// признаком завершения является равенство количества запросов и количества записанных файлов.
	if (FileManager.Exists(ExportTrendsPath) == false) FileManager.CreateRecursiveFolder(ExportTrendsPath);
	
	while (i <= Const.MAX_LINES)
	{
		tag = GetItemPath(i);
		FileDataSize[i] = 0;
		if (tag!="**" && GetLineVisible(ActiveMode,i)) 
		{
			FileSaveCount = FileSaveCount +1;  // Подсчет количества запросов на запись файлов
			fileName= Str.ToString(i)+".json";  // добавляем расширение
			
			fileName= FileManager.CombinePath(ExportTrendsPath, fileName);							// добавляем путь
			if (ActiveMode == Const.amOPERATIVE){					//сохранение  для оперативного режима
				if (i==1)	WS.RS.Main_Charts.Chart_Operative_mode.DataSource_1.Save(fileName,"JSON");		
				if (i==2)	WS.RS.Main_Charts.Chart_Operative_mode.DataSource_2.Save(fileName,"JSON");		
				if (i==3)	WS.RS.Main_Charts.Chart_Operative_mode.DataSource_3.Save(fileName,"JSON");		
				if (i==4)	WS.RS.Main_Charts.Chart_Operative_mode.DataSource_4.Save(fileName,"JSON");		
				if (i==5)	WS.RS.Main_Charts.Chart_Operative_mode.DataSource_5.Save(fileName,"JSON");		
				if (i==6)	WS.RS.Main_Charts.Chart_Operative_mode.DataSource_6.Save(fileName,"JSON");		
				if (i==7)	WS.RS.Main_Charts.Chart_Operative_mode.DataSource_7.Save(fileName,"JSON");		
				if (i==8)	WS.RS.Main_Charts.Chart_Operative_mode.DataSource_8.Save(fileName,"JSON");		
				if (i==9)	WS.RS.Main_Charts.Chart_Operative_mode.DataSource_9.Save(fileName,"JSON");		
				if (i==10)	WS.RS.Main_Charts.Chart_Operative_mode.DataSource_10.Save(fileName,"JSON");		
			}else{				// сохранение для исторического режима
				if (i==1)	WS.RS.Main_Charts.Chart_History_mode.DataSource_1.Save(fileName,"JSON");		
				if (i==2)	WS.RS.Main_Charts.Chart_History_mode.DataSource_2.Save(fileName,"JSON");		
				if (i==3)	WS.RS.Main_Charts.Chart_History_mode.DataSource_3.Save(fileName,"JSON");		
				if (i==4)	WS.RS.Main_Charts.Chart_History_mode.DataSource_4.Save(fileName,"JSON");		
				if (i==5)	WS.RS.Main_Charts.Chart_History_mode.DataSource_5.Save(fileName,"JSON");		
				if (i==6)	WS.RS.Main_Charts.Chart_History_mode.DataSource_6.Save(fileName,"JSON");		
				if (i==7)	WS.RS.Main_Charts.Chart_History_mode.DataSource_7.Save(fileName,"JSON");		
				if (i==8)	WS.RS.Main_Charts.Chart_History_mode.DataSource_8.Save(fileName,"JSON");		
				if (i==9)	WS.RS.Main_Charts.Chart_History_mode.DataSource_9.Save(fileName,"JSON");		
				if (i==10)	WS.RS.Main_Charts.Chart_History_mode.DataSource_10.Save(fileName,"JSON");		
			}
	//		pTrends.DebugTool_1.Log(TextEdit_1.Text+fileName);
		}
		i= i+1;
	}
	
	fileName= "00.json";  // 
	fileName= FileManager.CombinePath(ExportTrendsPath,fileName);							// добавляем путь
	ExportParams(FileSaveCount,fileName);
]]>
				</body>
