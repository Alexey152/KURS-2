	<object access-modifier="private" name="Item_ServerAPI" display-name="Item_ServerAPI" uuid="ccf78809-63e8-45ed-b2ad-6a36d678e2e9" base-type="ApItemString" base-type-id="11bc436c-015a-4db9-b901-0cc5c75e1239" ver="2">
		<init target="Source" ver="2" ref="ApSource_1"/>
		<init target="Path" ver="2" value="Service.InvokeFromJSON"/>
		<do-on access-modifier="private" name="Handler_1" display-name="Handler_1" ver="4" event="OnValueChanged">
			<body kind="javascript">
				<![CDATA[// #22657 - реализация алгоритма обращений к API сервера для получения относительного тега сигнала

var tagID;
if (Advised && value!="")		// Если сигнал подписан и значение определено
{
	var ask = JSON.parse( value );		// парсим строку в объект
	if (ask.transaction != null && ask.result != null)
	{
		if (ask.transaction.indexOf("GetID_") == 0)
		{									
			var index = ask.transaction.slice(6);			// Извлекаем из строки индекс графика
			if (ask.result.exception == null)	
			{
				Main_Panel.private.NowParams.TagID[index] = ask.result.return;		// Сохраняем результат в параметрах графика
				Main_Panel.private.SendRequestToGetParentObjectID(index);
			}
		}
		if (ask.transaction.indexOf("GetParentID_") == 0)
		{									
			var index = ask.transaction.slice(12);		
			if (ask.result.exception == null)	
			{
				Main_Panel.private.NowParams.ParentID[index] = ask.result.return;		
				Main_Panel.private.SendRequestToGetShortName(index);
			}
		}
		if (ask.transaction.indexOf("GetShortName_") == 0)
		{									
			var index = ask.transaction.slice(13);			
			if (ask.result.exception == null)	
			{
				var n = Main_Panel.private.NowParams.Tags[index].indexOf(ask.result.return);
				Main_Panel.private.NowParams.RelationTag[index] = Main_Panel.private.NowParams.Tags[index].slice(n);		
			}
			WS.RS.Main_Legend.ReFresh();
		}
			
//		DebugTool_1.Log("ItemValueChanged. Value= "+value);
	}
}


]]>
			</body>
		</do-on>
	</object>
