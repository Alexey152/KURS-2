			<body kind="om">
				<![CDATA[i:int4=0;

if (value == true)  // автоматический диапазон
{
	while (i <= Const.MAX_LINES)
	{
		WS.RS.Main_Charts.Chart_Operative_mode.Axis_Y[i].Scale = 2;
		WS.RS.Main_Charts.Chart_History_mode.Axis_Y[i].Scale = 2;	
		WS.RS.Main_Charts.Chart_History_mode.Axis_Y[20+i].Scale = 2;	// для данных загруженных из файла
		i = i +1;
	}
}
else					// фиксированный диапазон
{
	while (i <= Const.MAX_LINES)
	{
		WS.RS.Main_Charts.Chart_Operative_mode.Axis_Y[i].Scale = 0;
		WS.RS.Main_Charts.Chart_Operative_mode.Axis_Y[i].Max = Main_Panel.private.NowParams.Item_Ymax[i];
		WS.RS.Main_Charts.Chart_Operative_mode.Axis_Y[i].Min = Main_Panel.private.NowParams.Item_Ymin[i];
		WS.RS.Main_Charts.Chart_History_mode.Axis_Y[i].Scale = 0;
		WS.RS.Main_Charts.Chart_History_mode.Axis_Y[i].Max = Main_Panel.private.NowParams.Item_Ymax[i];
		WS.RS.Main_Charts.Chart_History_mode.Axis_Y[i].Min = Main_Panel.private.NowParams.Item_Ymin[i];
		WS.RS.Main_Charts.Chart_History_mode.Axis_Y[20+i].Scale = 0;
		WS.RS.Main_Charts.Chart_History_mode.Axis_Y[20+i].Max = Main_Panel.private.NowParams.Item_Ymax[20+i];
		WS.RS.Main_Charts.Chart_History_mode.Axis_Y[20+i].Min = Main_Panel.private.NowParams.Item_Ymin[20+i];
		i = i +1;
	}
}
]]>
			</body>
		</do-on>
	</object>
