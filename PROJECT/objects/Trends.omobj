						<do-on access-modifier="private" name="Handler_2" display-name="Handler_2" ver="4" event="OnRangeChange">
							<body kind="om">
								<![CDATA[if (!Main_Panel.private.bInit) return;
lb:uint8 = Variant.ToInt8(WS.RS.Main_Charts.Chart_Operative_mode.Axis_X.Min, 0);		// Левая граница интервала
rb:uint8 = Variant.ToInt8(WS.RS.Main_Charts.Chart_Operative_mode.Axis_X.Max, 0);		// Правая граница интервала
//DebugTool_1.Log("OnRangeChange "+Str.ToString(WS.RS.Main_Charts.Chart_Operative_mode.private.omSetInterval));
if (Timer_of_ReadData.Active == false) Main_Panel.private.ReFreshRedLine();
WS.RS.Main_Charts.Chart_Operative_mode.Update();//*/

dtnow:uint8 = DateTime.UtcNow();									// Текущее время

NewInterval:uint8 = rb - lb;											// Длина оперативного интервала
if (NewInterval != Main_Panel.private.tCurInterval && !WS.RS.Main_Charts.Chart_Operative_mode.private.omSetInterval)
{
	Main_Panel.private.tCurInterval = NewInterval;
//	DebugTool_1.Log("AxisX_Range : " + DateTime.ToString(Main_Panel.private.tCurInterval));
}

if (Main_Panel.private.tCurInterval>Main_Panel.private.tStoryInterval)	// Исли отображаемый интервал превысил хранимый интервал
{
	Main_Panel.private.tStoryInterval= Main_Panel.private.tCurInterval;		// Расширяем хранимый интервал
	WS.RS.Main_Charts.Chart_Operative_mode.private.omSetInterval = false;	// Сброс флага для запуска заполнения буфера историческими данными
	vu4:uint4 = TypeConvert.ToUint4( Main_Panel.private.tStoryInterval / Const.tONE_SECOND);  // Переводим интервал в секунды
	WS.RS.Main_Charts.Chart_Operative_mode.DataSource_1.ApItemValueQuery_1.OperativeInterval= vu4;
	WS.RS.Main_Charts.Chart_Operative_mode.DataSource_2.ApItemValueQuery_1.OperativeInterval= vu4;
	WS.RS.Main_Charts.Chart_Operative_mode.DataSource_3.ApItemValueQuery_1.OperativeInterval= vu4;
	WS.RS.Main_Charts.Chart_Operative_mode.DataSource_4.ApItemValueQuery_1.OperativeInterval= vu4;
	WS.RS.Main_Charts.Chart_Operative_mode.DataSource_5.ApItemValueQuery_1.OperativeInterval= vu4;
	WS.RS.Main_Charts.Chart_Operative_mode.DataSource_6.ApItemValueQuery_1.OperativeInterval= vu4;
	WS.RS.Main_Charts.Chart_Operative_mode.DataSource_7.ApItemValueQuery_1.OperativeInterval= vu4;
	WS.RS.Main_Charts.Chart_Operative_mode.DataSource_8.ApItemValueQuery_1.OperativeInterval= vu4;
	WS.RS.Main_Charts.Chart_Operative_mode.DataSource_9.ApItemValueQuery_1.OperativeInterval= vu4;
	WS.RS.Main_Charts.Chart_Operative_mode.DataSource_10.ApItemValueQuery_1.OperativeInterval= vu4;
}

if (WS.RS.Main_Charts.Chart_Operative_mode.private.omSetInterval)// Если изменение границ программное
{
	WS.RS.Main_Charts.Chart_Operative_mode.private.omSetInterval = false;	// сброс флага
//		DebugTool_1.Log("Переходим в оперативный режим");
}
else			// если изменение границ пользователем
{
	Main_Panel.private.deltaRight = rb - dtnow;												// Отклонение правой границы от текущего времени
	if (WS.RS.Main_Charts.Chart_Operative_mode.private.tMousePressed>0){	// При управлении с помощью минитрендов интервал таймером не меняется. 
		rb = rb +WS.RS.Main_Charts.Chart_Operative_mode.private.tMousePressed;	// Надо учитывать время выставления пользователем интервала
		WS.RS.Main_Charts.Chart_Operative_mode.private.tMousePressed = 0;
	}
	if (rb < dtnow)		// Если текущее время за пределами отображаемого интервала
	{
			// (#14927)Разрешать подгрузку истории = false, Использовать красную линию = false - 
			// Увеличился фрагмент графика где стоял курсор. При следующем обновлении графика фокус перемещается на область, где идет обновление графика.
		if (!PreloadHistoryToOperativeMode && !UseRedLine) return;	// При отключенной предазгрузке чтения истории не происходит
		Main_Panel.private.JointMode = 3;	// Блокируем работу СетИнтервал по таймеру
//		DebugTool_1.Log("Исторический режим");
		Main_Panel.private.SetPause(true);
			// (#14927)Разрешать подгрузку истории = false, Использовать красную линию = true - 
			// Увеличился фрагмент графика где стоял курсор. Обновление графика приостанавливается.
		if (!PreloadHistoryToOperativeMode ) return;	// При отключенной предазгрузке чтения истории не происходит
			//(#14927)Разрешать подгрузку истории = true, - Увеличился фрагмент графика где стоял курсор. 
			//Обновление графика приостанавливается. Отображаются исторические значения.
		if (lb<Main_Panel.private.LeftBorderStoryInterval){  
			t1:uint8 = lb ;
			t2:uint8 = lb + Main_Panel.private.tStoryInterval;
			Main_Panel.private.JointModeHistoryRequest(true, t1, t2);
//			DebugTool_1.Log("Исторический запрос: "+DateTime.ToString(t1)+"-"+DateTime.ToString(t2));
			return;
		}
		if (rb>Main_Panel.private.RightBorderStoryInterval){
			t2:uint8 = rb ;
			t1:uint8 = rb- Main_Panel.private.tStoryInterval;
			Main_Panel.private.JointModeHistoryRequest(true, t1, t2);
//			DebugTool_1.Log("Исторический запрос: "+DateTime.ToString(t1)+"-"+DateTime.ToString(t2));
			return;
		}
	}
	else
	{												// Если пользователь вернулся к текущему времени
//		DebugTool_1.Log("Оперативный  режим!");
		if (WS.RS.Main_Charts.Chart_Operative_mode.DataSource_1.ApItemValueQuery_1.Mode != Const.vqmOPERATIVE)
			Main_Panel.private.JointModeHistoryRequest(false, 0, 0);	// Переключаем смешанный режим в оперативный 
		if (Main_Panel.private.JointMode > 2){
			Main_Panel.private.JointMode= 1;	// Просто
			Main_Panel.private.SetPause(false);	// Если ДА, то сбрасываем паузу
		}
	}
}

//DebugTool_1.Log(Str.ToString(Timer_of_ReadData.Active)+" linesMin: "+Str.ToString(linesMin)+" lb: "+ Str.ToString(lb));


]]>
							</body>
						</do-on>
						<do-on access-modifier="private" name="Handler_3" display-name="Handler_3" ver="4" event="OnClickValue">
							<body kind="om">
								<![CDATA[//DebugTool_1.Log("Клик Х");

if(Main_Panel.private.CursorMode==3)//добавление репера
{
	i:uint4=Main_Panel.private.NextReper();
	WS.RS.Main_Charts.Chart_Operative_mode.ControlLine[i].Visible=true;
	WS.RS.Main_Charts.Chart_Operative_mode.ControlLine[i].Value = clickValue;
	WS.RS.Main_Charts.Chart_Operative_mode.ControlLine[i].setActive(true);
	WS.RS.Main_Charts.Chart_Operative_mode.private.lastAddReper=i;
	WS.RS.Main_Charts.Chart_Operative_mode.private.iActiveReper=i;
}
else		// Выделение репера
{
	// отклонение точки клика от репера инициализируем максимальным числом - видимый диапазон 
	dx:uint8 = Variant.ToUint8(WS.RS.Main_Charts.Chart_Operative_mode.Axis_X.Max - WS.RS.Main_Charts.Chart_Operative_mode.Axis_X.Min,0);
	dt:uint8 = Variant.ToUint8(dx/50,0);	// максимальное отклонение точки клика от репера определяем +/- 2% от диапазона
	clickMin:uint8 = Variant.ToUint8(clickValue-dt,0);	// минимальная граница диапазона в который должен был попасть репер
	clickMax:uint8 = Variant.ToUint8(clickValue+dt,0);	// максимальная граница диапазона в который должен попасть репер
	i:uint4=1;		
	n:uint4=TypeConvert.ToUint4(WS.RS.Main_Charts.Chart_Operative_mode.ControlLine.Length); // количество реперов
	bNeedAutoscaleOff:bool = true;
	while (i < n)
	{																													// перебираем реперы
		if (WS.RS.Main_Charts.Chart_Operative_mode.ControlLine[i].Visible){					// Если текущий репер видимый
			val:uint8 = Variant.ToUint8(WS.RS.Main_Charts.Chart_Operative_mode.ControlLine[i].Value,0);	// точка репера
			if ((val<clickMax) && (val>clickMin)){				// если точка репера попадает в границы диапазона +/-2% от точки клика
				vdx:uint8 = TypeConvert.ToUint8(Math.Abs(Variant.ToInt8(clickValue-val,0)));	// вычисляем отклонение репера от точки клика
				if (dx>vdx){ 																					// если отклонение репера меньше отклонения текущего репера
					WS.RS.Main_Charts.Chart_Operative_mode.ControlLine[i].setActive(true);	// делаем текущий репер активным
					WS.RS.Main_Charts.Chart_Operative_mode.private.iActiveReper=i;
					dx= vdx;																									// запоминаем отклонение выбранного репера
					bNeedAutoscaleOff = false;
				}
			}
		}
		i=i+1;
	}
	i=0;
	if (bNeedAutoscaleOff){
		while (i <= Const.MAX_LINES){				// для всех осей Y
			WS.RS.Main_Charts.Chart_Operative_mode.Axis_Y[i].Scale=0;	// отключаем автоматическое масштабирование
			i=i+1;
		}	// возврат к автоматическому масштабированию произойдет при использовании кнопки HOME.
	}
}//*/
//DebugTool_1.Log("OnClickValue_ReFresh");
WS.RS.Main_Legend.ReFresh();
