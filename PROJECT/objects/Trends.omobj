		<do access-modifier="private" name="RefreshStatus" display-name="RefreshStatus" ver="4" type="" type-id="">
			<body kind="om">
				<![CDATA[// Определяем содержимое строки статуса
//DebugTool_1.Log("Status "+Str.ToString(ApSource_1.ConnectionState)+" Item_"+Str.ToString(nAPItem));

if (ApSource_1.ConnectionState==2)	// Если источник подключен
{
	if (nAPItem==0)
		return;
		
	if (GetItemState(nAPItem)!=Const.stADVISED_OK && Main_Panel.private.GetItemPath(nAPItem)!="**")
	{
		Image_1.ImageFile = "Trends_Icons/trends_sign_warning.png";		
		Text_1 = "Не успешная подписка на тег "+Main_Panel.private.GetItemPath(nAPItem);
		CurrentApItem= nAPItem;	// Запоминаем сигнал по которому вывели инфу
		i: int4 = 1;
		Status_Panel.Image_1.Tooltip= "";
		while (i<=Const.MAX_LINES)
		{
			if (GetItemState(i)!=Const.stADVISED_OK && Main_Panel.private.GetItemPath(i)!="**")
			{
				Status_Panel.Image_1.Tooltip= Status_Panel.Image_1.Tooltip +Main_Panel.private.GetItemPath(i) +" : "+GetItemStateAsString(i) + "\n";
//				DebugTool_1.Log("@"+Status_Panel.Image_1.Tooltip);
			}
			i=i+1;
		}
	}
	else
	{
		if (GetItemState(CurrentApItem)==Const.stNOADVISED || Main_Panel.private.GetItemPath(CurrentApItem)=="**")
		{
			i: int4 = 1;
			CurrentApItem=0;
			Image_1.Tooltip= "";
			while (i<=Const.MAX_LINES)
			{
				if (GetItemState(i)!=Const.stADVISED_OK && Main_Panel.private.GetItemPath(i)!="**")
				{
					Image_1.ImageFile = "Trends_Icons/trends_sign_warning.png";
					Image_1.Tooltip= Main_Panel.private.GetItemPath(i) +" : "+GetItemStateAsString(i) + "\n";
					Text_1 = "Не успешная подписка на тег "+Main_Panel.private.GetItemPath(i);
					CurrentApItem= i;					
				}
				i=i+1;
			}
		}
		
		if (CurrentApItem==0)
		{
			Image_1.ImageFile = "";
			Image_1.Tooltip= "";
			Text_1 = "";
		}
	}
}
else
{
	Image_1.ImageFile = "Trends_Icons/trends_sign_ahtung.png";
	Image_1.Tooltip= ApSource_1.ErrorString;
	Text_1 = "Нет связи с источником";
	CurrentApItem=0;
}

// Изображение без корректного файла а браузере отображается иконкой битого файла,
// поэтому будем скрывать изображение когда нет ошибок или предупреждений
Image_1.Visible = Image_1.ImageFile != "";

Main_Panel.private.ReFresh_LegendTable.Invoke();
//DebugTool_1.Log("RefreshStatus "+Str.ToString(GetItemState(nAPItem)));
]]>
			</body>
