				<body kind="javascript">
					<![CDATA[
/*Table.Params = [];
[
["LinesCount"],	// 0 - Количество линий графиков для сохранения
["TInterval"],		// 1 - Временной интервал
["GlobalRanges"],	// 2 - Признак индивидуального масштабирования
["IndexAxis"],		// 3 - Номер оси привязанной к графику
["AxisMin"],			// 4 - Минимум диапазона значений по оси Y
["AxisMax"],			// 5 - Максимум диапазона значений по оси Y
["AxisColor"],		// 6 - Цвет осей Y
["LineColor"],		// 7 - Цвет линий
["Description"],	// 8 - Описание сигнала
["Tag"],				// 9 - Адрес сигнала
["Units"],			// 10- Единицы измерения сигнала
["LinesWidth"],	// 11- Толщина линии
["Values"],			// 12- Значения
["Time"],				// 13- Время 
["Quality"],			// 14- Качество
["Minimum"],			// 15- Минимум
["Maximum"],			// 16- Мксимум
["Quantity"],		// 17- Количество
["AutoRange"],		// 18- Режим автоматического подбора диапазона значений
["RepersCount"],	// 19- Количество реперов
["RepersValues"],	// 20- Привязка реперов
["ActiveMode"],	// 21- Текущий режим (0-оперативный, 1-исторический)
["THistInterval"],// 22- Временной интервал исторического режима
["ReperActive"]	// 23- Значение активного репера
]//*/

// Чтение из файла
if( FileManager.Exists( fileName ) )//проверка на существование файла
	{	
		var curContent = FileManager.ReadTextFile( fileName );
		var Table = JSON.parse( curContent );
		//запись настроек в таблице оперативного режима	
		ExParams.LinesCount= Table.LinesCount;
		ExParams.Xmin= Table.TInterval[0];
		ExParams.Xmax= Table.TInterval[1];
		ExParams.bGlobalRanges = Table.GlobalRanges;
		ExParams.Ymin[0]= Table.AxisMin[0];
		ExParams.Ymax[0]= Table.AxisMax[0];
		ExParams.Ycolor[0]= Table.AxisColor[0];
//		log(Table[4][1]+" json "+Table[5][1]);
//		log(ExParams.Ymin[0]+" f8 "+ExParams.Ymax[0]);
		var i;
		for (i=1;i<=ExParams.LinesCount;i++){
			ExParams.Item_numAxis[i]= Table.IndexAxis[i-1];
			ExParams.Ymin[i]= Table.AxisMin[i];
			ExParams.Ymax[i]= Table.AxisMax[i];
			ExParams.fixYmin[i]= Table.fixYMin[i];
			ExParams.fixYmax[i]= Table.fixYMax[i];
			ExParams.Advised[i]= Table.Advised[i-1];
			ExParams.Ycolor[i]= Table.AxisColor[i];
			ExParams.LineColor[i]= Table.LineColor[i-1];
			ExParams.Description[i]= Table.Description[i-1];
			ExParams.ItemTag[i]= Table.Tag[i-1];
			ExParams.ItemEU[i]= Table.Units[i-1];
			ExParams.LineWidth[i]= Table.LinesWidth[i-1];
			ExParams.LegendValues[i]= Table.Values[i-1];
			ExParams.LegendTime[i]= Table.Time[i-1];
			ExParams.LegendQuality[i]= Table.Quality[i-1];
			ExParams.LegendMin[i]= Table.Minimum[i-1];
			ExParams.LegendMax[i]= Table.Maximum[i-1];
			ExParams.LegendQuantity[i]= Table.Quantity[i-1];
			ExParams.nColumnT[i]=Table.nColumnT[i-1];
		}
		ExParams.bAutoRange = Table.AutoRange;
		ExParams.RepersCount = Table.RepersCount;
		for (i=0;i<ExParams.RepersCount;i++){
			ExParams.RepersValues[i]= Table.RepersValues[i];
		}
		ExParams.ActiveMode = Table.ActiveMode;
		ExParams.tHistRangeStart= Table.THistInterval[0];
		ExParams.tHistRangeEnd= Table.THistInterval[1];
		ExParams.ReperActiveValue= Table.ReperActive;
		for (i=0;i<12;i++){
			ExParams.LegendColumnWidth[i] = Table.LegendColumnWidth[i];
		}
		ExParams.LegendHeight= Table.LegendHeight;
		return true;
	}
	else 
	{
		return false;
	}]]>
