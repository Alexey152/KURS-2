LineHist[40]=WS.RS.Main_Charts.Chart_History_mode.Line_File_20; DecorHist[40]=WS.RS.Main_Charts.Chart_History_mode.Line_File_20.Decorator_1;

var index = Main_Panel.private.GetIndexRowSelected();		// фиксируем номер выделенной строки
if (index >= 0){			// Если строка таблицы выбрана и индекс в допустимом диапазоне
	index = Main_Panel.private.NowParams.iItemInLegend[index];  // Получаем индекс графика для выделенной строки
//	DebugTool_1.Log("Select "+index);
}

var i = 1;
while (i <= Const.MAX_LINES)
{
	if (index != i){		// Для остальных графиков
		LineOper[i].LineWidth = Main_Panel.private.NowParams.Line_Width[i];
		LineHist[i].LineWidth = Main_Panel.private.NowParams.Line_Width[i];
		DecorOper[i].LineWidth = Main_Panel.private.NowParams.Line_Width[i];
		DecorHist[i].LineWidth = Main_Panel.private.NowParams.Line_Width[i];	
	}else{			// Для выделенного графика
		var vw = Main_Panel.private.NowParams.Line_Width[i] + Main_Panel.private.deltaLineWidth//"Увеличение при выделении";
		LineOper[i].LineWidth = vw;
		LineHist[i].LineWidth = vw;
		DecorOper[i].LineWidth = vw;
		DecorHist[i].LineWidth = vw;	
//		WS.RS.Main_Charts.Chart_Operative_mode.private.zOrderCount = WS.RS.Main_Charts.Chart_Operative_mode.private.zOrderCount +1;
		LineOper[i].zOrder = WS.RS.Main_Charts.Chart_Operative_mode.private.zOrderCount;
		LineHist[i].zOrder = WS.RS.Main_Charts.Chart_Operative_mode.private.zOrderCount;
		
	}
//		log(LineOper[i].Decorator_1.Color+"="+LineOper[i].Color);
	i = i +1;
}

i=21;	// Для данных из файлов
while (i <= 40)
{
	if (index!=(i)){		// Для остальных графиков
		LineHist[i].LineWidth = Main_Panel.private.NowParams.Line_Width[i];
		DecorHist[i].LineWidth = Main_Panel.private.NowParams.Line_Width[i];	
	}else{			// Для выделенного графика
		var vw = Main_Panel.private.NowParams.Line_Width[i] + Main_Panel.private.deltaLineWidth//"Увеличение при выделении";
		LineHist[i].LineWidth = vw;
		DecorHist[i].LineWidth = vw;	
//		WS.RS.Main_Charts.Chart_Operative_mode.private.zOrderCount = WS.RS.Main_Charts.Chart_Operative_mode.private.zOrderCount +1;
		LineHist[i].zOrder = WS.RS.Main_Charts.Chart_Operative_mode.private.zOrderCount;
		
	}
	i = i +1;
}

WS.RS.Main_Charts.Chart_Operative_mode.Update();
WS.RS.Main_Charts.Chart_History_mode.Update();
]]>
