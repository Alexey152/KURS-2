	nItem:int4=0;		// начальное значение для поиска свободного датасурса
	while (i <= Const.MAX_LINES)
	{
		fileName= Str.ToString(i)+".json";
		fileName= FileManager.CombinePath(Main_Panel.private.ExportTrendsPath, fileName);
		if (FileManager.Exists(fileName))		// если очередной файл для загрузки существует
		{	
//			nItem= NextItem(nItem);		// поиск свободного датасурса для загрузки в него очередного файла
			nItem= i;  // Последовательная загрузка датасурсов
			if (nItem==1) WS.RS.Main_Charts.Chart_History_mode.DataSource_File_1.Load(fileName,"json");
			if (nItem==2) WS.RS.Main_Charts.Chart_History_mode.DataSource_File_2.Load(fileName,"json");
			if (nItem==3) WS.RS.Main_Charts.Chart_History_mode.DataSource_File_3.Load(fileName,"json");
			if (nItem==4) WS.RS.Main_Charts.Chart_History_mode.DataSource_File_4.Load(fileName,"json");
			if (nItem==5) WS.RS.Main_Charts.Chart_History_mode.DataSource_File_5.Load(fileName,"json");
			if (nItem==6) WS.RS.Main_Charts.Chart_History_mode.DataSource_File_6.Load(fileName,"json");
			if (nItem==7) WS.RS.Main_Charts.Chart_History_mode.DataSource_File_7.Load(fileName,"json");
			if (nItem==8) WS.RS.Main_Charts.Chart_History_mode.DataSource_File_8.Load(fileName,"json");
			if (nItem==9) WS.RS.Main_Charts.Chart_History_mode.DataSource_File_9.Load(fileName,"json");
			if (nItem==10) WS.RS.Main_Charts.Chart_History_mode.DataSource_File_10.Load(fileName,"json");
			Main_Panel.private.FileLoadCount= Main_Panel.private.FileLoadCount +1;
		}
