					<![CDATA[// Поиск доступного репера для установки
i:int4 = 1;
n:int4 = TypeConvert.ToInt4( WS.RS.Main_Charts.Chart_Operative_mode.ControlLine.Length-1);
if (ActiveMode==0)		// для оперативного режима
{
	// перебираем реперы в поисках свободного (невидимого)
	while (i<n && WS.RS.Main_Charts.Chart_Operative_mode.ControlLine[i].Visible==true) {i=i+1;}
	if (i==n){	// Проверяем последний репер
		if (WS.RS.Main_Charts.Chart_Operative_mode.ControlLine[i].Visible==true){// если все реперы уже установлены
			i= WS.RS.Main_Charts.Chart_Operative_mode.private.lastAddReper +1;
			if (i>n) i=1;
		}
	}
}
else			// для исторического режима
{
	while (i<n && WS.RS.Main_Charts.Chart_History_mode.ControlLine[i].Visible==true) {i=i+1;}
	if (i==n){	// если все реперы уже установлены
		if (WS.RS.Main_Charts.Chart_History_mode.ControlLine[i].Visible==true){
			i= WS.RS.Main_Charts.Chart_History_mode.private.lastAddReper +1;
			if (i>n) i=1;
		}
	}
}
//DebugTool_1.Log("NextReper "+Str.ToString(i)+" "+Str.ToString(n));
return i;
]]>
