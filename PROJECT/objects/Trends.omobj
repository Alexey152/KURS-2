	<do access-modifier="private" name="LoadConfiguration" display-name="LoadConfiguration" ver="4" type="bool" type-id="7c9fe1ab-1ee2-42d8-939f-94ce3e89fca8">
		<body kind="javascript">
			<![CDATA[Main_Panel.private.bConfigLoaded=true; // Запоминем что начальная загрузка конфигурации состоялась
Main_Panel.private.Initializer();// инициализатор выполняется только один раз 
// определение пути записи файла
var fileName = filePath;		//  В качестве параметра в функцию должен передаваться только путь без имени файла
if (fileName==""){
	fileName = FileManager.CombinePath(FileManager.ProjectFolder,"resources");
	fileName = FileManager.CombinePath(fileName,"Trends_Settings");
	if( !FileManager.Exists( fileName ))	{	
		FileManager.CreateRecursiveFolder( fileName );
	}
}
fileName = FileManager.CombinePath( fileName, "trendsview.json"); 
fileName = Main_Panel.private.TestRelationPath( fileName ); // Проверка и дополнение относительного пути

