	<do access-modifier="private" name="SetGraphicAxis" display-name="SetGraphicAxis" ver="3" type="" type-id="">
		<param access-modifier="private" name="tag" display-name="" uuid="7d37dad2-0394-49db-aedc-3c2e4b7ce9fe" base-type="string" base-type-id="76403785-f3d5-41a7-9eb6-d19d2aa2d95d" ver="2"/>
		<param access-modifier="private" name="axisName" display-name="" uuid="fd00baa3-5f78-4b4d-b551-e7231e578804" base-type="string" base-type-id="76403785-f3d5-41a7-9eb6-d19d2aa2d95d" ver="2"/>
		<body kind="om">
			<![CDATA[Main_Panel.private.Initializer();// инициализатор выполняется только один раз 
if (axisName!="" && tag!="")
{
	n0:int4 = 0; 
	i:int4=1;
	nAxis:int4=0;											// индекс графика к которому мы привязыаемся
	while(i<=Main_Panel.private.MAX_LINES){
		if (Main_Panel.private.GetItemPath(i)==axisName) nAxis=i;
		if (Main_Panel.private.GetItemPath(i)==tag) n0=i;
		i=i+1;
	}
	if (nAxis==0||n0==0){
		DebugTool_1.Log("SetGraphicAxis. Индекс для одного из параметров не определен!");
		return;
	}	

	id:int4 = WS.RS.Main_Charts.Chart_Operative_mode.Axis_Y[nAxis].GetID();  // получаем идентификатор шкалы
	WS.RS.Main_Charts.Chart_Operative_mode.Axis_Y[nAxis].Visible = true;
	if (n0==1) WS.RS.Main_Charts.Chart_Operative_mode.Line_1.SetAxisById(id); // делаем привязку к новой шкале
	if (n0==2) WS.RS.Main_Charts.Chart_Operative_mode.Line_2.SetAxisById(id);
	if (n0==3) WS.RS.Main_Charts.Chart_Operative_mode.Line_3.SetAxisById(id);
	if (n0==4) WS.RS.Main_Charts.Chart_Operative_mode.Line_4.SetAxisById(id);
	if (n0==5) WS.RS.Main_Charts.Chart_Operative_mode.Line_5.SetAxisById(id);
	if (n0==6) WS.RS.Main_Charts.Chart_Operative_mode.Line_6.SetAxisById(id);
	if (n0==7) WS.RS.Main_Charts.Chart_Operative_mode.Line_7.SetAxisById(id);
	if (n0==8) WS.RS.Main_Charts.Chart_Operative_mode.Line_8.SetAxisById(id);
	if (n0==9) WS.RS.Main_Charts.Chart_Operative_mode.Line_9.SetAxisById(id);
	if (n0==10) WS.RS.Main_Charts.Chart_Operative_mode.Line_10.SetAxisById(id);
	id = WS.RS.Main_Charts.Chart_History_mode.Axis_Y[nAxis].GetID();  // получаем идентификатор шкалы
	WS.RS.Main_Charts.Chart_History_mode.Axis_Y[nAxis].Visible = true;
	if (n0==1) WS.RS.Main_Charts.Chart_History_mode.Line_1.SetAxisById(id); // делаем привязку к новой шкале
	if (n0==2) WS.RS.Main_Charts.Chart_History_mode.Line_2.SetAxisById(id);
	if (n0==3) WS.RS.Main_Charts.Chart_History_mode.Line_3.SetAxisById(id);
	if (n0==4) WS.RS.Main_Charts.Chart_History_mode.Line_4.SetAxisById(id);
	if (n0==5) WS.RS.Main_Charts.Chart_History_mode.Line_5.SetAxisById(id);
	if (n0==6) WS.RS.Main_Charts.Chart_History_mode.Line_6.SetAxisById(id);
	if (n0==7) WS.RS.Main_Charts.Chart_History_mode.Line_7.SetAxisById(id);
	if (n0==8) WS.RS.Main_Charts.Chart_History_mode.Line_8.SetAxisById(id);
	if (n0==9) WS.RS.Main_Charts.Chart_History_mode.Line_9.SetAxisById(id);
	if (n0==10) WS.RS.Main_Charts.Chart_History_mode.Line_10.SetAxisById(id);
	prevAxis:int4 = Main_Panel.private.NowParams.Item_numAxis[n0];
	Main_Panel.private.NowParams.Item_numAxis[n0]=nAxis;     // запоминем индекс новой шкалы для данного графика
	
	i=1;  //  проверяем в цикле нужна ли еще другим графикам старая шкала
	bNeed:bool = false;
	while (i<=10){
		if (Main_Panel.private.NowParams.Item_numAxis[i]==prevAxis) bNeed=true;
		WS.RS.Main_Charts.Chart_Operative_mode.Axis_Y[i].AxisName= ""; // Очищаем все имена осей, чтобы сформировать заново
		i=i+1;
	}
	if (!bNeed){
		 WS.RS.Main_Charts.Chart_Operative_mode.Axis_Y[prevAxis].Visible = false;
		 WS.RS.Main_Charts.Chart_History_mode.Axis_Y[prevAxis].Visible = false;
	}
	i=1;
}
WS.RS.Main_Legend.ReFresh();
]]>
		</body>
	</do>
