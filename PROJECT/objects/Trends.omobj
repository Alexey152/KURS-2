			<do access-modifier="private" name="ReFreshLayoutWeight" display-name="ReFreshLayoutWeight" ver="4" type="" type-id="">
				<body kind="om">
					<![CDATA[if (RecInit.Visible)
	return;

if (ShowToolbar)
{
	// Если отображение панели инструментов включено
	if (Main_Panel.LayoutWeight_1 != 32) Main_Panel.LayoutWeight_1 = 32;		// В качестве веса используется размер панели в пикселах
	Main_Panel.Visible = true;
	Main_Panel.ToolBar.Visible = true;
}
else
{
	// Если отображение панели инструментов вЫключено
	if (Main_Panel.LayoutWeight_1 != 0) Main_Panel.LayoutWeight_1 = 0;		
	Main_Panel.Visible = false;
	Main_Panel.ToolBar.Visible = false;
}

if (ShowStatus)
{
	// Если отображение строки статуса включено
	if (Status_Panel.LayoutWeight_1 != 20) Status_Panel.LayoutWeight_1 = 20;		// В качестве веса используется размер панели в пикселах
	Status_Panel.Visible = false;
}
else
{	
	// Если отображение строки статуса вЫключено
	if (Status_Panel.LayoutWeight_1 != 0) Status_Panel.LayoutWeight_1 = 0;
	Status_Panel.Visible = true;
}

// Основная панель с учетом видимости строки статуса и панели инструментов
wsHeight: double = here.Height - Main_Panel.LayoutWeight_1 - Status_Panel.LayoutWeight_1;
if (WS.LayoutWeight_1 != wsHeight)
	WS.LayoutWeight_1 = wsHeight;
WS.UpdateLayout();

if (ShowLegend==false) 
{
	// Если легенда скрыта
	if (WS.RS.Main_Legend.LayoutWeight_1 != 0) WS.RS.Main_Legend.LayoutWeight_1 = 0;
	WS.RS.Main_Legend.Table_1.Visible = false;
	if (WS.RS.Slider.LayoutWeight_1 != 0)	WS.RS.Slider.LayoutWeight_1 = 0;
}
else
{
	// Если легенда отображается
	if (!WS.RS.Main_Legend.Table_1.Visible) WS.RS.Main_Legend.Table_1.Visible = true;
	if (!WS.RS.Main_Legend.Visible) WS.RS.Main_Legend.Visible = true;
	nRows:uint4 ;
	if (AutoSizeLegend==true){	// Если включено автомасштабирование легенды
		nRows = WS.RS.Main_Legend.Table_1.TableModel_1.GetRowCount();	// Определяем количество строк в легенде
		if (nRows<1) nRows = 1;																// Проверяем минимальный размер легенды по строкам
		if (nRows>MaxLineLegendShow) nRows = MaxLineLegendShow;	// Проверяем максимальный размер легенды по строкам
		vf:double = 18+WS.RS.Main_Legend.Table_1.HeaderHeight+nRows*WS.RS.Main_Legend.Table_1.RowHeight;
		if (WS.RS.Main_Legend.LayoutWeight_1 != vf) WS.RS.Main_Legend.LayoutWeight_1 = vf ;	// Вычисляем размер легенды и используем его в качестве веса
	}
//	if (WS.RS.Main_Legend.LayoutWeight_1<101) WS.RS.Main_Legend.LayoutWeight_1 = 101;  // минимальный размер таблицы
	if (WS.RS.Slider.LayoutWeight_1 != 3) WS.RS.Slider.LayoutWeight_1 = 3;
}

vf: double = WS.LayoutWeight_1 - WS.RS.Main_Legend.LayoutWeight_1 	- WS.RS.Slider.LayoutWeight_1;
if (vf<0) vf = 0;
if (WS.RS.Main_Charts.LayoutWeight_1 != vf) WS.RS.Main_Charts.LayoutWeight_1 = vf;

]]>
				</body>
			</do>
