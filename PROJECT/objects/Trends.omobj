			<![CDATA[Main_Panel.private.Initializer();  // инициализатор выполняется только один раз 
// определение пути записи файла
if (fileName == "")
{
  var FolderPath = FileManager.CombinePath("export", "taglist");
	FolderPath = FileManager.CombinePath(FileManager.ProjectFolder, FolderPath);
	var FName="TagList.json";
	fileName = FileManager.CombinePath( FolderPath, FName); 
	if ( !FileManager.Exists( FolderPath ))	{	
		FileManager.CreateRecursiveFolder( FolderPath );
	}
}
fileName = Main_Panel.private.TestRelationPath( fileName ); // Проверка и дополнение относительного пути

var i=1,n;
while (i <= Const.MAX_LINES)
{
	if (Main_Panel.private.GetItemPath(i) != "**") n=i;
	i = i +1;
}
Main_Panel.private.ExportParams(n,fileName);

if (FileManager.Exists( fileName )) return true;
else return false;
]]>
