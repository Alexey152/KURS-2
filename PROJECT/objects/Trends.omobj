						<![CDATA[var jsonTag = data.GetText();
var structTag = JSON.parse(jsonTag);
var i=0;
while (i<structTag.data.length)
{
	if (structTag.data[i].tag!=null)		// поле tag должно быть обязательно
	{						
		ForcedAddItem(structTag.data[i].tag, structTag.data[i].eunits, structTag.data[i].description);
		if (structTag.data[i].Color!=null)
			SetGraphicColorU4(structTag.data[i].tag,structTag.data[i].Color);
		var AxisName = structTag.data[i].tag;
		if (structTag.data[i].Axis!=null)
		{
			AxisName = structTag.data[i].Axis;
			SetGraphicAxis(structTag.data[i].tag,AxisName);
		}
		if (structTag.data[i].AxisColor!=null)
			SetAxisColorU4(AxisName,structTag.data[i].Color);
		var j= Main_Panel.private.GetIndexByPath(AxisName);
		if (j>0 && structTag.data[i].LowRange!=null)
			Main_Panel.private.NowParams.Item_Ymin[j] = structTag.data[i].LowRange;
		if (j>0 && structTag.data[i].HighRange!=null)
			Main_Panel.private.NowParams.Item_Ymax[j] = structTag.data[i].HighRange;
	}
	i=i+1;
}
if (ActiveMode==1) Main_Panel.private.HistoryReFresh();	// Для исторического режима автоматически запрашиваем историю
AutoRange = AutoRange;]]>
