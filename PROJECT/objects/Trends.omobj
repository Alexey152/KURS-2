			<do access-modifier="private" name="ExportParams" display-name="ExportParams" ver="4" type="" type-id="">
				<body kind="javascript">
					<![CDATA[//var fileName = File.CombinePath(ExportTrendsPath,"00.json");
var Items=[];
Items[1] = Item_1;
Items[2] = Item_2;
Items[3] = Item_3;
Items[4] = Item_4;
Items[5] = Item_5;
Items[6] = Item_6;
Items[7] = Item_7;
Items[8] = Item_8;
Items[9] = Item_9;
Items[10] = Item_10;
var Lines=[];
Lines[1]= WS.RS.Main_Charts.Chart_History_mode.Line_1;
Lines[2]= WS.RS.Main_Charts.Chart_History_mode.Line_2;
Lines[3]= WS.RS.Main_Charts.Chart_History_mode.Line_3;
Lines[4]= WS.RS.Main_Charts.Chart_History_mode.Line_4;
Lines[5]= WS.RS.Main_Charts.Chart_History_mode.Line_5;
Lines[6]= WS.RS.Main_Charts.Chart_History_mode.Line_6;
Lines[7]= WS.RS.Main_Charts.Chart_History_mode.Line_7;
Lines[8]= WS.RS.Main_Charts.Chart_History_mode.Line_8;
Lines[9]= WS.RS.Main_Charts.Chart_History_mode.Line_9;
Lines[10]= WS.RS.Main_Charts.Chart_History_mode.Line_10;
var j = fileName.lastIndexOf("/");
if (j < 1) j = fileName.lastIndexOf("\\");
var filePath = fileName.substring(0,j);
if (FileManager.Exists(filePath)==false) FileManager.CreateRecursiveFolder(filePath);

var Table = {
	"LinesCount":0,		// 0 - Количество линий графиков для сохранения
	"TInterval":[],		// 1 - Временной интервал оперативного режима
	"GlobalRanges":0,	// 2 - Признак индивидуального масштабирования
	"IndexAxis":[],		// 3 - Номер оси привязанной к графику
	"AxisMin":[],			// 4 - Реальные Минимум диапазона значений по оси Y
	"AxisMax":[],			// 5 - Реальные Максимум диапазона значений по оси Y
	"fixYMin":[],			// Минимум фиксированного диапазона значений
	"fixYMax":[],			// Максимум фиксированного диапазона значений
	"Advised":[],
	"AxisColor":[],		// 6 - Цвет осей Y
	"LineColor":[],		// 7 - Цвет линий
	"Description":[],	// 8 - Описание сигнала
	"Tag":[],				// 9 - Адрес сигнала	
	"Units":[],				// 10- Единицы измерения сигнала
	"LinesWidth":[],		// 11- Толщина линии
	"Values":[],			// 12- Значения
	"Time":[],				// 13- Время 
	"Quality":[],			// 14- Качество
	"Minimum":[],			// 15- Минимум
	"Maximum":[],			// 16- Мксимум
	"Quantity":[],		// 17- Количество
	"AutoRange":0,		// 18- Режим автоматического подбора диапазона значений
	"RepersCount":0,	// 19- Количество реперов
	"RepersValues":[],	// 20- Привязка реперов
	"ActiveMode":0,		// 21- Текущий режим (0-оперативный, 1-исторический)
	"THistInterval":[],// 22- Временной интервал исторического режима
	"ReperActive":0,		// 23- Значение активного репера	
	"nColumnT":[],			// Колонка таблицы определяющая время (сервер\источник)
	"LegendColumnWidth":[], // Ширина колонок таблицы легенды
	"LegendHeight":100	// Высота таблицы легенды
}//*/

if (FileManager.Exists(filePath))
{
	Table.GlobalRanges = !Main_Panel.ToolBar.btnIndiZoom.Checked.Value;
	Table.TInterval.push(WS.RS.Main_Charts.Chart_Operative_mode.Axis_X.Min, WS.RS.Main_Charts.Chart_Operative_mode.Axis_X.Max);
	Table.THistInterval.push(WS.RS.Main_Charts.Chart_History_mode.Axis_X.Min, WS.RS.Main_Charts.Chart_History_mode.Axis_X.Max);
	if (ActiveMode == Const.amOPERATIVE) {
		Table.AxisMin.push(WS.RS.Main_Charts.Chart_Operative_mode.Axis_Y[0].Min);
		Table.AxisMax.push(WS.RS.Main_Charts.Chart_Operative_mode.Axis_Y[0].Max);
	}else{
		Table.AxisMin.push(WS.RS.Main_Charts.Chart_History_mode.Axis_Y[0].Min);
		Table.AxisMax.push(WS.RS.Main_Charts.Chart_History_mode.Axis_Y[0].Max);
	}
	Table.AxisColor.push(WS.RS.Main_Charts.Chart_Operative_mode.Axis_Y[0].Color);
	var i=1,j=1;	
	Table.fixYMin.push(LowRange);		// По нулевому индексу сохраняем общие значения фиксированного диапазона, 
	Table.fixYMax.push(HighRange);	//	по остальным индивидуальные
	while ((i <= Const.MAX_LINES) && (j <= linesCount))
	{
		while ((Main_Panel.private.GetItemPath(i)=="**")&&(i<=10)){ i=i+1; } // Пропускаем линии котрые не подписаны на данные
		Table.IndexAxis.push(NowParams.Item_numAxis[i]);
		Table.fixYMin.push(Main_Panel.private.NowParams.Item_Ymin[i]);	
		Table.fixYMax.push(Main_Panel.private.NowParams.Item_Ymax[i]);
		if (ActiveMode == Const.amOPERATIVE) {
			Table.AxisMin.push(WS.RS.Main_Charts.Chart_Operative_mode.Axis_Y[i].Min);
			Table.AxisMax.push(WS.RS.Main_Charts.Chart_Operative_mode.Axis_Y[i].Max);
			Table.nColumnT.push(2);
		}else{
			Table.AxisMin.push(WS.RS.Main_Charts.Chart_History_mode.Axis_Y[i].Min);
			Table.AxisMax.push(WS.RS.Main_Charts.Chart_History_mode.Axis_Y[i].Max);		
			Table.nColumnT.push(Lines[i].ColumnNumberTime);
		}
		Table.Advised.push(WS.RS.Main_Legend.Table_1.TableModel_1.GetCellData(j-1,Const.cnADVISE));
//		Table.AxisColor.push(WS.RS.Main_Charts.Chart_Operative_mode.Axis_Y[i].Color);
		var vv = WS.RS.Main_Legend.Table_1.TableModel_1.GetCellData(j-1, Const.cnCL_AXIS);
		Table.AxisColor.push(vv);
		Table.LineColor.push(Lines[i].Color);
		Table.Description.push(WS.RS.Main_Legend.Table_1.TableModel_1.GetCellData(j-1,Const.cnDESCRIPTION));
		Table.Tag.push(Items[i].Path);
		Table.Units.push(WS.RS.Main_Legend.Table_1.TableModel_1.GetCellData(j-1,Const.cnEU));
		Table.LinesWidth.push(NowParams.Line_Width[i]);
		Table.Values.push(WS.RS.Main_Legend.Table_1.TableModel_1.GetCellData(j-1,Const.cnVALUE));
		Table.Time.push(WS.RS.Main_Legend.Table_1.TableModel_1.GetCellData(j-1,Const.cnTIME));
		Table.Quality.push(WS.RS.Main_Legend.Table_1.TableModel_1.GetCellData(j-1,Const.cnQUALITY));
		Table.Minimum.push(WS.RS.Main_Legend.Table_1.TableModel_1.GetCellData(j-1,Const.cnMINIMUM));
		Table.Maximum.push(WS.RS.Main_Legend.Table_1.TableModel_1.GetCellData(j-1,Const.cnMAXIMUM));
		Table.Quantity.push(WS.RS.Main_Legend.Table_1.TableModel_1.GetCellData(j-1,Const.cnQUANTITY));
		j	=	j	+1;	// подсчет количества записанных данных
		i	=	i	+1;
	}
	Table.LegendColumnWidth.push(WS.RS.Main_Legend.Table_1.TableColumn_0.Width);
	Table.LegendColumnWidth.push(WS.RS.Main_Legend.Table_1.TableColumn_1.Width);
	Table.LegendColumnWidth.push(WS.RS.Main_Legend.Table_1.TableColumn_2.Width);
	Table.LegendColumnWidth.push(WS.RS.Main_Legend.Table_1.TableColumn_3.Width);
	Table.LegendColumnWidth.push(WS.RS.Main_Legend.Table_1.TableColumn_4.Width);
	Table.LegendColumnWidth.push(WS.RS.Main_Legend.Table_1.TableColumn_5.Width);
	Table.LegendColumnWidth.push(WS.RS.Main_Legend.Table_1.TableColumn_6.Width);
	Table.LegendColumnWidth.push(WS.RS.Main_Legend.Table_1.TableColumn_7.Width);
	Table.LegendColumnWidth.push(WS.RS.Main_Legend.Table_1.TableColumn_8.Width);
	Table.LegendColumnWidth.push(WS.RS.Main_Legend.Table_1.TableColumn_9.Width);
	Table.LegendColumnWidth.push(WS.RS.Main_Legend.Table_1.TableColumn_10.Width);
	Table.LegendColumnWidth.push(WS.RS.Main_Legend.Table_1.TableColumn_11.Width);
	Table.LegendHeight=WS.RS.Main_Legend.Height;
	Table.AutoRange=!Main_Panel.ToolBar.btnAutoRange.Checked.Value;	// Сохраняем состояние кнопки автомасштабирования
																		// использовать свойство типа AutoRange здесь не получается - уведомляющий тип JS не поддерживает
	Table.LinesCount = j -1; // записываем сколько реально сохранено данных
	i=0; j=0;
	while (i <= 20) // Сохранение параметров реперов
	{				
		if (ActiveMode == Const.amOPERATIVE)
		{
			if (WS.RS.Main_Charts.Chart_Operative_mode.ControlLine[i].Visible)
			{
				var vv = WS.RS.Main_Charts.Chart_Operative_mode.ControlLine[i].Value;
				Table.RepersValues.push(vv); 
				if (WS.RS.Main_Charts.Chart_Operative_mode.ControlLine[i].getActive())
					Table.ReperActive=vv;
				j= j +1;
			}
		}
