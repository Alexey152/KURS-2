  FolderPath:string = FileManager.CombinePath("export","taglist");
	FolderPath = FileManager.CombinePath(FileManager.ProjectFolder, FolderPath);
	FName:string = "TagList.json";
	fileName = FileManager.CombinePath( FolderPath, FName); 
}
fileName = Main_Panel.private.TestRelationPath( fileName ); // Проверка и дополнение относительного пути

if (Main_Panel.private.ImportParams(fileName))  // Загружаем параметры сипска сигналов в набор переменных Main_Panel.private.ExParams
{
	i:int4= 1;
	while (i<=Main_Panel.private.ExParams.LinesCount){
		ForcedAddItem(Main_Panel.private.ExParams.ItemTag[i],Main_Panel.private.ExParams.ItemEU[i],Main_Panel.private.ExParams.Description[i]);
		i=i+1;
	}
	Main_Panel.private.ApplyExParams();
//	ActiveMode = Main_Panel.private.ExParams.ActiveMode;  // режим не восстанавливаем в соответсвии с задачей №17144
	vt:uint8= Main_Panel.private.ExParams.tHistRangeStart +Main_Panel.private.TimeZone;
	Main_Panel.ToolBar.RecHistory.DateEdit_1.Text = Main_Panel.private.DateToString(vt);
	Main_Panel.ToolBar.RecHistory.TimeEdit_1.Text = Main_Panel.private.TimeToString(vt);
	vt= Main_Panel.private.ExParams.tHistRangeEnd +Main_Panel.private.TimeZone;
	Main_Panel.ToolBar.RecHistory.DateEdit_2.Text = Main_Panel.private.DateToString(vt);
	Main_Panel.ToolBar.RecHistory.TimeEdit_2.Text = Main_Panel.private.TimeToString(vt);
//	DebugTool_1.Log(Main_Panel.ToolBar.RecHistory.TextEdit_1.Text);
	FitToScreen();
	return true;
}
else
{
	DebugTool_1.Log("Trends.LoadSignalList.Ошибка чтения");
	return false;
}

